{
  "name": "ezanalyze",
  "version": "0.0.0",
  "homepage": "https://rufeeoh.github.io/EZ-Analyze-Repository",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
  "emulators": "npm --prefix functions run build && firebase emulators:start",
    "start": "concurrently -k -n EMULATORS,ANGULAR -c yellow,green \"npm:emulators\" \"npm:serve\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:rules": "firebase emulators:exec --only firestore 'node tests/firestore/run.js'",
    "test:rules:attach": "FIRESTORE_EMULATOR_HOST=localhost:8080 node tests/firestore/run.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.1.5",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "firebase": "^11.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.7",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "concurrently": "^8.2.2",
    "firebase-tools": "^13.0.0",
    "@firebase/rules-unit-testing": "^4.0.0"
  }
}