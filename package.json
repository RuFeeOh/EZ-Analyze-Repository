{
  "name": "ezanalyze",
  "version": "0.0.0",
  "homepage": "https://rufeeoh.github.io/EZ-Analyze-Repository",
  "scripts": {
    "ng": "ng",
    "preemulators": "node scripts/cleanup-ports.mjs",
    "serve": "ng serve",
    "emulators": "npm --prefix functions run build && node scripts/cleanup-ports.mjs && firebase emulators:start --inspect-functions",
    "start": "concurrently -k -n EMULATORS,ANGULAR -c yellow,green \"npm:emulators\" \"npm:serve\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:rules": "firebase emulators:exec --only firestore 'node tests/firestore/run.js'",
    "test:rules:attach": "FIRESTORE_EMULATOR_HOST=localhost:8080 node tests/firestore/run.js",
    "deploy:functions": "npm --prefix functions run build && firebase deploy --only functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.2.3",
    "@angular/material": "^20.2.1",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.2.3",
    "@angular/router": "^20.2.3",
    "@angular/service-worker": "^20.2.3",
    "firebase": "^11.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "@firebase/rules-unit-testing": "^4.0.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "firebase-tools": "^14.15.1",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}